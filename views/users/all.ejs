<!doctype html>
<html>
<head>
     <% include ../partials/header %>
</head>
<body>
Welcome <%= currentUser.local.email %>, here are the registered users <p>

<% for(var i = 0; i < users.length; i++) { %>

<div class="col-sm-6 col-md-4">
    <div class="thumbnail">
        <img src="<%= users[i].facebook.photo %>">
        <div class="caption">
            <h3><%= users[i].facebook.name %></h3>
        </div>
    </div>
</div>

<% } %>


</body>
</html>